{
    "rules": {
        "no-duplicate-variable": true,
        "no-unused-variable": [
            true
        ],
        "indent": [
            true,
            "spaces",
            4
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type"
        ]
    },
    "rulesDirectory": [
        "node_modules/tslint-eslint-rules/dist/rules"
    ]
}
